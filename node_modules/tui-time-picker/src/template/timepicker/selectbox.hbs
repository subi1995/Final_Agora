<select class="tui-timepicker-select" aria-label="Time">
    {{#each items as |item|}}
        {{#if ([../helpers/equals] ../initialValue item)}}
            <option value="{{item}}" selected {{lookup ../disabledItems @index}}>{{[../helpers/timeFormat] item ../format}}</option>
        {{else}}
            <option value="{{item}}" {{lookup ../disabledItems @index}}>{{[../helpers/timeFormat] item ../format}}</option>
        {{/if}}
    {{/each}}
</select>
